{
    "name": "oh-my-type",
    "version": "2.0.0",
    "description": "Oh my type! Tiny and robust type checker.",
    "main": "build/index.js",
    "scripts": {
        "dev": "tsc -w",
        "build": "tsc -b",
        "test": "jest src/index.test.ts",
        "version:major": "npm version major --git-tag-version=false",
        "version:minor": "npm version minor --git-tag-version=false",
        "version:patch": "npm version patch --git-tag-version=false",
        "git:add": "git add .",
        "git:commit": "npm run git:add && git commit",
        "git:push": "git push origin/master",
        "release:major": "npm run version:major && npm run git:commit && npm run git:push",
        "release:minor": "npm run version:minor && npm run git:commit && npm run git:push",
        "release:patch": "npm run version:patch && npm run git:commit && npm run git:push"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Neporotovskiy/Oh-My-Type.git"
    },
    "author": "Anton Neporotovskiy",
    "license": "MIT",
    "keywords": [
        "javascript",
        "types",
        "utility",
        "isString",
        "isNumber",
        "isBoolean",
        "isUndefined",
        "isNull",
        "isNaN",
        "isObject",
        "isArray",
        "isSymbol",
        "isFunction"
    ],
    "devDependencies": {
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.18",
        "jest": "^24.9.0",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.3"
    }
}
